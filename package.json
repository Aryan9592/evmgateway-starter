{
  "name": "optimism-starter",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "anvil": "source .env && anvil --fork-url $ANVIL_FORK_URL --chain-id $ANVIL_CHAIN_ID",
    "deploy:OPVerifier": "source .env && forge create contracts/src/op-verifier/OPVerifier.sol:OPVerifier --rpc-url $FORGE_RPC_URL_GOERLI -i --etherscan-api-key $ETHERSCAN_API_KEY_GOERLI --verify --constructor-args $OP_GATEWAY_URLS $OUTPUT_ORACLE_ADDRESS",
    "deploy:L2TestCoin": "source .env && forge create contracts/src/L2TestCoin.sol:L2TestCoin --rpc-url $FORGE_RPC_URL_OP_GOERLI -i --etherscan-api-key $ETHERSCAN_API_KEY_OP_GOERLI --verify --constructor-args \"L2 Test Coin\" \"L2TESTCOIN\" 6",
    "deploy:L2TestNFT": "source .env && forge create contracts/src/L2TestNFT.sol:L2TestNFT --rpc-url $FORGE_RPC_URL_OP_GOERLI -i --etherscan-api-key $ETHERSCAN_API_KEY_OP_GOERLI --verify --constructor-args \"L2 Test NFT\" \"L2TESTNFT\"",
    "deploy:L1PassportNFT": "source .env && forge create contracts/src/L1PassportNFT.sol:L1PassportNFT --rpc-url $FORGE_RPC_URL_GOERLI -i --etherscan-api-key $ETHERSCAN_API_KEY_GOERLI --verify --constructor-args \"L1 Passport NFT\" \"L1PASSPORT\" $OP_VERIFIER_CONTRACT_ADDRESS 0x5a81f1f4d30f4153150848c31fabd0311946ed72 0x22299910e573ecd436f4987c75f093894904d107",
    "deploy": "source .env && forge create contracts/src/Counter.sol:Counter --rpc-url $FORGE_RPC_URL --private-key $FORGE_PRIVATE_KEY --etherscan-api-key $ETHERSCAN_API_KEY --verify",
    "deploy:anvil": "source .env && forge create contracts/src/AttestationStation.sol:AttestationStation --rpc-url http://127.0.0.1:8545 --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80 --legacy",
    "dev": "vite",
    "dev:foundry": "npm run dev & wagmi generate --watch & npm run anvil",
    "build": "npm run wagmi && vite build",
    "build:production": "vite build",
    "preview": "vite preview",
    "lint": "prettier --write \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ensdomains/thorin": "^0.6.44",
    "@eth-optimism/contracts-bedrock": "^0.16.2",
    "@openzeppelin/contracts": "4.7.3",
    "@openzeppelin/contracts-upgradeable": "4.7.3",
    "@rainbow-me/rainbowkit": "^1.0.7",
    "buffer": "^6.0.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-state": "^2.1.1",
    "styled-components": "^6.1.0",
    "util": "^0.12.4",
    "viem": "^1.18.1",
    "wagmi": "^1.3.9"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^3.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}
